<!DOCTYPE html>
<html lang="en">
<head>

    <%- include('component/head') %>
    <title>{{ TITLE }}</title>
    <link href="/lib/datatables/media/css/jquery.dataTables.min.css" rel="stylesheet"/>
    <!--<link href="/lib/datatables/media/css/dataTables.bootstrap.min.css" rel="stylesheet"/>-->
</head>
<body>

<div class="wrapper">

    <%- include('component/sidebar') %>

    <div class="main-panel">

        <%- include('component/navbar', { breadcumb : "Dashboard > Log Alert" } ) %>


        <div class="content">
            <div class="container-fluid">

            	<!-- MAIN CONTENT -->
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Log Alert</h4>
                                <!--<p class="category">Here is a subtitle for this table</p>-->
                            </div>
                            <div class="content">
                                <table class="table table-hover" id="table-log" style="margin: 10px">
                                    <thead>
                                    <th width="200px">Date</th>
                                    <th>ID Device</th>
                                    <th>Patient</th>
                                    <th>Status</th>
                                    </thead>
                                    <tbody>
                                    <% for (var i = 0; i <20; i++) { %>
                                    <tr>
                                        <td>20 October 2016, 19:20</td>
                                        <td>AZ299102</td>
                                        <td>Kiswanto D2</td>
                                        <td><div class="label label-success">Success Sended</div></td>
                                    </tr>
                                    <% } %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>

        <%- include('component/footer') %>

    </div>
</div>

    <%- include('component/body_js') %>
    <script src="/lib/datatables/media/js/jquery.dataTables.min.js"></script>
    <script>
        $(document).ready(function() {
            $('#table-log').DataTable();
        } );
    </script>
</body>

</html>
